<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="SoftoneBundle\Entity\CustomerIrs" repository-class="SoftoneBundle\Entity\CustomerIrsRepository" table="softone_customerirs">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
    
        <field name="name" type="string" column="supplier" length="255" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="address" type="string" column="address" length="255" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>

        <field name="district" type="string" column="district" length="255" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="zip" type="string" column="zip" length="255" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>               
                     
        <field name="ts" type="datetime" column="ts" nullable="true">
            <options>
        
            </options>
        </field>
        <field name="actioneer" type="integer" column="actioneer" nullable="true">
            <options>
                <option name="unsigned"/>
            </options>
        </field>
        <field name="created" type="datetime" column="created" nullable="true"/>
        <field name="modified" type="datetime" column="modified" nullable="true"/>
    </entity>
</doctrine-mapping>
