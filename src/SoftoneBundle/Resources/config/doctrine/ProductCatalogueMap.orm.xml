<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SoftoneBundle\Entity\ProductCatalogueMap" table="softone_product_catalogue_map">
    <indexes>
      <index name="catalogue" columns="catalogue"/>
      <index name="catalogue_2" columns="catalogue"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="catalogue" type="integer" column="catalogue" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attribute" type="string" column="attribute" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="csvfield" type="integer" column="csvfield" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
