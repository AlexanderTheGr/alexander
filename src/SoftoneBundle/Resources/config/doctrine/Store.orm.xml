<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SoftoneBundle\Entity\Store" table="softone_store">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="series" type="integer" column="series" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="field" type="string" column="field" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="field2" type="string" column="field2" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rafi" type="string" column="rafi" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ts" type="datetime" column="ts" nullable="false">
      <options>
        
      </options>
    </field>
    <field name="actioneer" type="integer" column="actioneer" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
  </entity>
</doctrine-mapping>
