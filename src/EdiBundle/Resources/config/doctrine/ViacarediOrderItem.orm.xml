<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EdiBundle\Entity\ViacarediOrderItem" repository-class="EdiBundle\Entity\ViacarediOrderItemRepository"  table="viacaredi_order_item">
    <indexes>
      <index name="viacarediorder" columns="viacarediorder"/>
      <index name="viacaredi" columns="viacaredi"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="viacarediorderitem" columns="viacaredi,viacarediorder"/>
    </unique-constraints>    
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one
        field="viacaredi"
        target-entity="Viacaredi"
        inversed-by="Viacaredi"
        join-column="viacaredi">
        <join-column name="viacaredi" referenced-column-name="id" />
    </many-to-one>   
      
    <many-to-one
        field="ViacarediOrder"
        target-entity="ViacarediOrder"
        inversed-by="ViacarediOrderItem"
        join-column="viacarediorder">
        <join-column name="viacarediorder" referenced-column-name="id" />
    </many-to-one> 
    
            
    <field name="qty" type="integer" column="qty" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="price" type="decimal" column="price" precision="10" scale="2" nullable="false"/>
    <field name="discount" type="decimal" column="discount" precision="10" scale="2" nullable="false"/>
    <field name="fprice" type="decimal" column="fprice" precision="10" scale="2" nullable="false"/>
    <field name="store" type="integer" column="store" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">7021</option>
      </options>
    </field>
    <field name="chk" type="boolean" column="chk" nullable="false"/>
   
  </entity>
</doctrine-mapping>
