    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reference" type="integer" column="reference" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="supplierCode" type="string" column="supplier_code" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierTrdrSupfindataLbal" type="string" column="supplier_trdr_supfindata_lbal" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierName" type="string" column="supplier_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierAfm" type="string" column="supplier_afm" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierAddress" type="string" column="supplier_address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierDistrict" type="string" column="supplier_district" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierZip" type="string" column="supplier_zip" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierCity" type="string" column="supplier_city" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierPhone01" type="string" column="supplier_phone01" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierPhone02" type="string" column="supplier_phone02" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierFax" type="string" column="supplier_fax" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierWebpage" type="string" column="supplier_webpage" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="supplierUpddate" type="datetime" column="supplier_upddate" nullable="false"/>
    <field name="supplierInsdate" type="datetime" column="supplier_insdate" nullable="false"/>
    <field name="ts" type="datetime" column="ts" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="actioneer" type="integer" column="actioneer" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
    <field name="flatData" type="text" column="flat_data" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
