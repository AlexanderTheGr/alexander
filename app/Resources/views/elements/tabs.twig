{% block body %}
<div class="alexander {{ctrl}}" ng-app="{{app}}" ng-controller="{{ctrl}}">
    <div class="card">
        <div class="card-head">
            <ul class="nav nav-tabs" data-toggle="tabs">
                <li ng-repeat="tab in tabs" class="[[tab.active]]" ><a href="#[[tab.index]]">[[ tab.name ]]</a></li>
            </ul>
        </div><!--end .card-head -->
        <div class="card-body tab-content">
            <div  ng-repeat="tab in tabs" ng-bind-html="deliberatelyTrustDangerousSnippet(tab.content)"  class="tab-pane [[tab.active]]" id="[[tab.index]]">
                    
                </div>
            </div><!--end .card-body -->
        </div><!--end .card -->
    </div><!--end .col -->
    <!-- END LAYOUT LEFT ALIGNED -->
{% endblock %}

