{% extends 'base.html.twig' %}

{% block body %}
    <div id="wrapper">
        <div id="container">
            <div id="welcome">
                <h1><span>Καλως ήρθατε</span> στο Parts Box της FastWeb</h1>
            </div>

        </div>
    </div>
    <div class="col-lg-12">
        <h2 class="text-primary">Charts</h2>
    </div><!--end .col -->
    <div class="col-lg-12">
        <h4>Examples</h4>
    </div><!--end .col -->
    <div class="col-lg-3 col-md-4">
        <div class="card">
            <div class="card-body">
                <div class="column">
                    <div id="rickshawDemo2"></div>
                    <div id="slider"></div>
                </div>
            </div><!--end .card-body -->
        </div><!--end .card -->
    </div><!--end .col -->
    <div class="col-lg-offset-1 col-md-4">
        <div class="card">
            <div class="card-body">
                <div id="rickshawGraph" class="height-4" data-color1="#0aa89e" data-color2="rgba(79, 89, 89, 0.2)"></div>
            </div><!--end .card-body -->
        </div><!--end .card -->
    </div><!--end .col -->
    <div class="col-lg-offset-1 col-sm-4">
        <div class="card">
            <div class="card-body text-center">
                <div class="knob knob-accent knob-default-track size-4"><input type="text" class="dial" data-min="0" data-max="365" data-cursor=true value="90"></div>
            </div><!--end .card-body -->
        </div><!--end .card -->
        <em class="text-caption">Interactive knob chart</em>
        <div class="card">
            <div class="card-body text-center">
                <div class="knob knob-primary knob-default-light-track size-4"><input type="text" class="dial" value="56" data-angleOffset=-125 data-angleArc=250 data-readOnly=true></div>
            </div><!--end .card-body -->
        </div><!--end .card -->
        <em class="text-caption">Gauge style knob chart</em>
    </div><!--end .col -->    
{% endblock %}

{% block javascripts %}
    <script>


    </script>

{% endblock %}